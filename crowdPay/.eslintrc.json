{
  "extends": "airbnb-base",
  "env": {
      "node": true
  },
  "rules": {
    "strict": "off",
    "arrow-spacing": 0,
    "padded-blocks": 0,
    "key-spacing": 0,
    "space-infix-ops":0,
    "space-in-parens":0,
    "object-curly-spacing":0,
    "semi": [
      "warn",
      "never"
    ]

  }
}
