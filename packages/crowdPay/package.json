{



  "name": "crowdpay",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=local sls invoke local -f $fn  -p tests/events/$fn.json --watch",
    "test": "NODE_ENV=local node -r ./tests/utils.js node_modules/.bin/jest",
    "docs": "lambda-docs-2md ./lib",
    "push": "git add -A && git commit -m \"$msg\" && git push origin $(git rev-parse --abbrev-ref HEAD)",
    "submit": "curl --silent -u $(git config user.name) -d '{ \"title\": \"'$(git rev-parse --abbrev-ref HEAD)'\", \"head\": \"'$(git rev-parse --abbrev-ref HEAD)'\", \"base\": \"master\", \"body\":\"'\"$body\"'\"}' https://api.github.com/repos/iot-course/org/pulls"
  },
  "devDependencies": {
    "aws-sdk": "^2.188.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "dotenv": "^5.0.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-iot-course": "^0.0.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.1.4",
    "lambda-docs-2md": "^0.0.4",
    "serverless": "^1.26.0",
    "serverless-dynamodb-local": "^0.2.27",
    "serverless-webpack": "^4.2.0",
    "sync-request": "^5.0.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
